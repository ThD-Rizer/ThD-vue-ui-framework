{
  "name": "@thd/vue-ui-framework",
  "version": "0.27.0",
  "description": "UI framework for Vue.js",
  "author": "ThD.Rizer <thd.rizer@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "build": "yarn build:icons && yarn build:lib",
    "build:icons": "node builders/icons/index.js",
    "build:demo": "vue-cli-service build --dest dist-demo ./demo/index.js",
    "build:lib": "vue-cli-service build --target lib --name ui ./src/lib.js && rimraf ./dist/demo.html",
    "build:report": "vue-cli-service build --target lib ./src/lib.js --report",
    "build:publish": "yarn lint && yarn build && yarn publish",
    "dev": "vue-cli-service serve --port 5000 ./demo/index.js"
  },
  "dependencies": {
    "body-scroll-lock": "3.0.3",
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "4.5.0",
    "lodash.padstart": "4.6.1",
    "lodash.shuffle": "4.2.0",
    "lodash.throttle": "4.1.1",
    "normalize.scss": "0.1.0",
    "vue-markdown": "2.2.4",
    "vuejs-datepicker": "1.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.1",
    "@babel/plugin-proposal-optional-chaining": "7.10.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-plugin-router": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "@vue/eslint-config-airbnb": "5.1.0",
    "autoprefixer": "9.8.4",
    "babel-eslint": "10.1.0",
    "eslint": "7.3.1",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-vue": "6.2.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "postcss-flexbugs-fixes": "4.2.1",
    "raw-loader": "4.0.1",
    "rimraf": "3.0.2",
    "sass": "1.26.9",
    "sass-loader": "8.0.2",
    "stylelint": "13.6.1",
    "stylelint-config-standard-scss": "1.1.0",
    "stylelint-scss": "3.18.0",
    "vue": "2.6.11",
    "vue-router": "3.3.4",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0"
  },
  "resolutions": {
    "@babel/parser": "7.7.5"
  },
  "publishConfig": {
    "registry": "http://npm.thd-dev.ru/"
  },
  "files": [
    "dist/",
    "src/",
    "package.json",
    "README.md"
  ],
  "main": "dist/ui.umd.js",
  "module": "dist/ui.common.js",
  "repository": {
    "url": "git@gitlab.com:thd.rizer_projects/personal/thd-vue-ui-framework.git",
    "type": "git"
  }
}
