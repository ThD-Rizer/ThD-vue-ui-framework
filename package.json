{
  "name": "@thd/vue-ui-framework",
  "version": "0.15.1",
  "description": "UI framework for Vue.js",
  "author": "ThD.Rizer <thd.rizer@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "yarn build:icons && yarn build:lib",
    "build:icons": "node build/icons/index.js",
    "build:lib": "vue-cli-service build --target lib --name ui ./src/lib.js",
    "build:demo": "vue-cli-service build ./demo/index.js",
    "build:report": "vue-cli-service build --target lib ./src/lib.js --report",
    "build:publish": "yarn lint && yarn build && yarn publish",
    "dev": "vue-cli-service serve --port 5000 ./demo/index.js"
  },
  "dependencies": {
    "body-scroll-lock": "3.0.1",
    "lodash.isequal": "4.5.0",
    "lodash.padstart": "4.6.1",
    "normalize.scss": "0.1.0",
    "vuejs-datepicker": "1.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-proposal-pipeline-operator": "7.8.3",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-plugin-router": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-airbnb": "5.0.2",
    "autoprefixer": "9.7.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-vue": "6.2.2",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "postcss-flexbugs-fixes": "4.2.0",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "vue": "2.6.11",
    "vue-router": "3.1.6",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.42.1"
  },
  "resolutions": {
    "@babel/parser": "7.7.5"
  },
  "publishConfig": {
    "registry": "http://npm.thd-dev.ru/"
  },
  "files": [
    "dist/",
    "src/",
    "package.json",
    "README.md"
  ],
  "main": "dist/ui.umd.js",
  "module": "dist/ui.common.js",
  "repository": {
    "url": "git@gitlab.com:thd.rizer_projects/personal/thd-vue-ui-framework.git",
    "type": "git"
  }
}
